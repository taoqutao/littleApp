@import "/component/tableview/cell/cell.wxss";
button {
  width: 100%;
  height: 88rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0;
}

button::after {
  border: none;
}

.btn {
  width: 660rpx;
  height: 82rpx;
  margin-top: 38rpx;
  margin-left: 45rpx; 
  color: white;
  background-color: #ED605A;
  font-size: 40rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}